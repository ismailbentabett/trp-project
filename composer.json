{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "abbasudo/laravel-purity": "^1.0",
        "algolia/algoliasearch-client-php": "^3.4",
        "amendozaaguiar/filament-route-statistics": "^1.4",
        "awcodes/filament-curator": "^3.0",
        "bilfeldt/laravel-route-statistics": "^2.2",
        "brunocfalcao/blade-feather-icons": "^3.0",
        "cloudinary-labs/cloudinary-laravel": "^2.0",
        "coderflex/laravel-ticket": "^1.5",
        "digikraaft/laravel-review-rating": "^3.0",
        "filament/filament": "^3.0.49",
        "guava/filament-icon-picker": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.8",
        "laravel/breeze": "^1.23",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.3",
        "laravel/socialite": "^5.9",
        "laravel/tinker": "^2.8",
        "laravolt/avatar": "^5.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "leandrocfe/filament-apex-charts": "3.0",
        "mohammad-fouladgar/eloquent-builder": "^4.3",
        "mydnic/laravel-subscribers": "^1.9",
        "nesbot/carbon": "^2.72",
        "openai-php/client": "^0.7.3",
        "openai-php/laravel": "^0.7.0",
        "owenvoke/blade-fontawesome": "^2.4",
        "pbmedia/laravel-ffmpeg": "^8.3",
        "rtconner/laravel-likeable": "^3.2",
        "shayanys/lara-reserve": "^2.0",
        "solution-forest/filament-tree": "^2.0",
        "sopamo/laravel-filepond": "^1.2",
        "spatie/laravel-permission": "^5.11",
        "stephenjude/filament-blog": "^2.0",
        "symfony/http-client": "^6.3",
        "symfony/mailgun-mailer": "^6.3",
        "tightenco/ziggy": "^1.0",
        "tucker-eric/eloquentfilter": "^3.2",
        "z3d0x/filament-fabricator": "^2.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/attributes": "^2.4",
        "laravel-lang/common": "^4.0",
        "laravel-lang/http-statuses": "^3.4",
        "laravel-lang/lang": "^13.2",
        "laravel-lang/publisher": "^14.7",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.25",
        "laravel/telescope": "^4.17",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
